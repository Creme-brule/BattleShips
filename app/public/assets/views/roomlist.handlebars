<div class="container">
  <div class="popUp" id="roomPopUp">


<ul>
  {{#each rooms}} {{> rooms/room-block}} {{else}}
  <h3>NO ROOMS TO JOIN</h3>
  <button class="waves-effect waves-light btn black" id="createRoom" type="submit">CREATE ROOM</button>
  {{/each}}
</ul>

  </div>
  <div class="dash">
    <a href="/dashboard">
      <button class="btn-floating btn-large waves-effect waves green darken-1" id="toDash">
        <i class="material-icons">dashboard</i>
        Dashboard</button>
    </a>
  </div>
  <div class="logout">
    <a href="/logout">
      <button class="btn-floating btn-large waves-effect waves red darken-1" id="logoutRoom">
        <i class="material-icons">exit_to_app</i>
        Log Out</button>
    </a>
  </div>
</div>

<script>
  var createable = true;
  $("#createRoom").on("click", function () {
    if (createable) {
      createable = false;
      $.ajax({
        url: "/api/new",
        type: "POST",
        data: {
          player1Id: localStorage.getItem("userId")
        }
      }).then(function (data) {
        location.href = "/map/" + data;
      });
    }
  });

</script>
<script src="/js/roomlist.js" type="text/javascript"></script>